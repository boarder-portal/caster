{
  "name": "caster",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "run-p client:watch deno-server:watch css:watch rtmp:run",
    "start:prod": "run-p client:build deno-server:run rtmp:run",
    "client:watch": "parcel watch index.html --no-cache --cache-dir .parcel-cache --dist-dir ./public --public-url /public",
    "client:build": "parcel build index.html --cache-dir .parcel-cache --dist-dir ./public --public-url /public",
    "server:run": "npm run ts:run app/server/index.ts",
    "deno-server:watch": "denon run --unstable --import-map deno-import-map.json --allow-run --allow-env --allow-write --allow-read --allow-net app/deno-server/index.ts",
    "deno-server:run": "deno run --unstable --import-map deno-import-map.json --allow-run --allow-net --allow-write --allow-read --allow-net app/deno-server/index.ts",
    "rtmp:run": "npm run ts:run app/rtmp-server/index.ts",
    "css:watch": "tcm -p app/**/*.pcss --watch",
    "lint": "eslint . --ext .ts --ext .tsx",
    "lint:fix": "eslint . --ext .ts --ext .tsx --fix",
    "ts:check": "tsc --noEmit",
    "ts:run": "ts-node-dev --transpile-only -r tsconfig-paths/register"
  },
  "alias": {
    "client": "./app/client",
    "server": "./app/server",
    "shared": "./app/shared"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@parcel/transformer-less": "^2.0.0",
    "@parcel/validator-typescript": "^2.0.1",
    "@types/classnames": "^2.3.1",
    "@types/koa": "^2.13.4",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-static": "^4.0.2",
    "@types/lodash": "^4.14.177",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^9.8.8",
    "eslint": "^7.32.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "postcss-nested": "^4.2.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.4.4"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "flv.js": "^1.6.2",
    "koa": "^2.13.4",
    "koa-compose": "^4.1.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.6",
    "node-media-server": "^2.3.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.0",
    "recoil": "^0.5.0"
  }
}
